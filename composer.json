{
    "name": "subjective-php/psr-cache-redis",
    "description": "PSR-16 SimpleCache Implementation using Predis",
    "keywords": ["psr16", "psr", "cache", "simple-cache", "redis", "predis"],
    "license": "MIT",
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.0",
        "predis/predis": "^1.1",
        "psr/simple-cache": "^1.0",
        "subjective-php/psr-cache-helper": "^1.0"
    },
    "require-dev": {
        "php-coveralls/php-coveralls": "^2.1",
        "phpunit/phpunit": "^6.1",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "SubjectivePHP\\Psr\\SimpleCache\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SubjectivePHPTest\\Psr\\SimpleCache\\": "tests"
        }
    }
}
